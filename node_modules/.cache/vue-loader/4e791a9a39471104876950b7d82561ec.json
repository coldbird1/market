r\n      return this.cartList.every((value) => {\r\n        return value.checked == true;\r\n      });\r\n    },\r\n    totalPrice() {\r\n      return this.cartList\r\n        .filter((value) => {\r\n          return value.checked == true;\r\n        })\r\n        .reduce((total, value) => {\r\n          let price = parseFloat(value.newPrice.slice(1));\r\n          return total + value.count * price;\r\n        }, 0);\r\n    },\r\n    totalcount() {\r\n      return this.cartList\r\n        .filter((value) => {\r\n          return value.checked == true;\r\n        })\r\n        .reduce((total, value) => {\r\n          return total + value.count;\r\n        }, 0);\r\n    },\r\n  },\r\n  watch: {},\r\n  methods: {\r\n    allClick() {\r\n      let temp = this.cartList.every((value) => {\r\n        return value.checked == true;\r\n      });\r\n\r\n      if (temp) {\r\n        this.cartList.forEach((value) => {\r\n          value.checked = false;\r\n        });\r\n      } else {\r\n        this.cartList.forEach((value) => {\r\n          value.checked = true;\r\n        });\r\n      }\r\n    },\r\n  },\r\n  created() {},\r\n  mounted() {},\r\n};\r\n</script>\r\n<style scoped>\r\n.left {\r\n  background-color: black;\r\n  width: 72%;\r\n  color: #fff;\r\n}\r\n\r\nimg {\r\n  width: 18px;\r\n  height: 18px;\r\n  border-radius: 50%;\r\n  border: 1px solid gray;\r\n  background-color: #fff;\r\n}\r\n.selecter {\r\n  position: relative;\r\n  bottom: -4px;\r\n  left: 10px;\r\n}\r\n.all {\r\n  margin-left: 20px;\r\n  margin-right: 50px;\r\n}\r\n.right {\r\n  width: 28%;\r\n  background-color: orange;\r\n  text-align: center;\r\n  color: black;\r\n}\r\n.active {\r\n  background-color: red;\r\n  border: 1px solid red;\r\n}\r\n</style>"]}]}